{
  "manifest_version": 2,
  "name": "Catppuccin-Latte-Yellow Thunderbird Theme",
  "version": "1.0.0",
  "applications": {
    "gecko": {
      "id": "elkrien@gmail.com",
      "strict_min_version": "60.0"
    }
  },
  "description": "Catppuccin theme for Thunderbird - Latte Yellow",
